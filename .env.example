# Daytona Environment Configuration
# Copy this file to .env and adjust values as needed

# Default package version for development
DEFAULT_PACKAGE_VERSION=0.0.0-dev

# Package publishing configuration (leave empty for local development)
PYPI_PKG_VERSION=
NPM_PKG_VERSION=
NPM_TAG=
PYPI_TOKEN=
NPM_TOKEN=

# Database configuration (used by docker-compose)
POSTGRES_PASSWORD=pass
POSTGRES_USER=user
POSTGRES_DB=daytona

# Redis configuration
REDIS_URL=redis://localhost:6379

# OpenTelemetry configuration
OTEL_ENABLED=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318

# MinIO configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# DEX configuration
DEX_ISSUER=http://localhost:5556/dex

# API configuration
API_PORT=3000
API_URL=http://localhost:3000

# Dashboard configuration
DASHBOARD_PORT=3001
DASHBOARD_URL=http://localhost:3001

# Proxy configuration
PROXY_PORT=4000

# Runner configuration  
RUNNER_PORT=3003

# SSH Gateway configuration
SSH_GATEWAY_PORT=2222

# Development configuration
NODE_ENV=development