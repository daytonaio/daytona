{
  "name": "docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/docs",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "astro dev --host"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "astro build"
      },
      "configurations": {
        "production": {
          "command": "astro build --prod"
        }
      },
      "dependsOn": ["update-llms", "update-search"]
    },
    "update-cli-reference": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}/tools",
        "command": "node ./update-cli-reference.js"
      }
    },
    "update-api-reference": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}/tools",
        "command": "node ./update-api-reference.js"
      },
      "dependsOn": ["api:openapi"]
    },
    "update-llms": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}/tools",
        "command": "node ./update-llms.js"
      }
    },
    "update-search": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}/tools",
        "command": "node ./update-search.js"
      }
    },
    "check-version-env": {},
    "docker": {
      "options": {
        "target": "docs",
        "build-args": [
          "VERSION=$VERSION",
          "PUBLIC_ALGOLIA_APP_ID=$PUBLIC_ALGOLIA_APP_ID",
          "PUBLIC_ALGOLIA_API_KEY=$PUBLIC_ALGOLIA_API_KEY"
        ]
      }
    },
    "push-manifest": {}
  },
  "implicitDependencies": ["api"]
}
