# Daytona Runner Android Configuration
# Cuttlefish Virtual Device (CVD) Runner Environment Variables
# Copy this file to .env and update the values

ENVIRONMENT=development

# Logging
LOG_LEVEL=debug

# Daytona API Configuration
SERVER_URL=http://localhost:3000/api
API_PORT=3007
API_TOKEN=secret_api_token_3007

# SSH Gateway (for connecting to sandboxes)
SSH_GATEWAY_ENABLE=false
SSH_PUBLIC_KEY=""
SSH_HOST_KEY_PATH=/workspaces/daytona/tmp/runner/id_rsa

# =============================================================================
# Cuttlefish Virtual Device Configuration
# =============================================================================

# Remote CVD host configuration (SSH mode)
# Leave CVD_SSH_HOST empty for local mode
CVD_SSH_HOST=root@142.132.220.119
CVD_SSH_KEY_PATH=/workspaces/daytona/.tmp/ssh/id_rsa

# Cuttlefish Paths
CVD_INSTANCES_PATH=/var/lib/cuttlefish/instances
CVD_ARTIFACTS_PATH=/var/lib/cuttlefish/artifacts
CVD_HOME=/home/vsoc-01

# Cuttlefish Binary Paths
CVD_LAUNCH_PATH=/home/vsoc-01/bin/launch_cvd
CVD_STOP_PATH=/home/vsoc-01/bin/stop_cvd
CVD_PATH=/home/vsoc-01/bin/cvd
CVD_ADB_PATH=adb

# Port Configuration
# ADB port = CVD_ADB_BASE_PORT + instance_num
# WebRTC port = CVD_WEBRTC_BASE_PORT + instance_num
CVD_ADB_BASE_PORT=6520
CVD_WEBRTC_BASE_PORT=8443

# Instance Management
CVD_BASE_INSTANCE=1
CVD_MAX_INSTANCES=100

# Default VM Resources
CVD_DEFAULT_CPUS=4
CVD_DEFAULT_MEMORY_MB=8192
CVD_DEFAULT_DISK_GB=20

# =============================================================================
# Optional: TLS Configuration
# =============================================================================
# ENABLE_TLS=false
# TLS_CERT_FILE=/path/to/cert.pem
# TLS_KEY_FILE=/path/to/key.pem

# =============================================================================
# Polling Configuration (v2 API)
# =============================================================================
# API_VERSION=2
# POLL_TIMEOUT=30s
# POLL_LIMIT=10
# HEALTHCHECK_INTERVAL=30s
# HEALTHCHECK_TIMEOUT=10s
