        },
        "volumeId": {
          "type": "string"
        }
      }
    },
    "enums.SandboxState": {
      "type": "string",
      "enum": [
        "creating",
        "restoring",
        "destroyed",
        "destroying",
        "started",
        "stopped",
        "starting",
        "stopping",
        "resizing",
        "error",
        "unknown",
        "pulling_image"
      ],
      "x-enum-varnames": [
        "SandboxStateCreating",
        "SandboxStateRestoring",
        "SandboxStateDestroyed",
        "SandboxStateDestroying",
        "SandboxStateStarted",
        "SandboxStateStopped",
        "SandboxStateStarting",
        "SandboxStateStopping",
        "SandboxStateResizing",
        "SandboxStateError",
        "SandboxStateUnknown",
        "SandboxStatePullingImage"
      ]
    },
    "enums.SnapshotState": {
      "type": "string",
      "enum": ["NONE", "PENDING", "IN_PROGRESS", "COMPLETED", "FAILED"],
      "x-enum-varnames": [
        "SnapshotStateNone",
        "SnapshotStatePending",
        "SnapshotStateInProgress",
        "SnapshotStateCompleted",
        "SnapshotStateFailed"
      ]
    }
  },
